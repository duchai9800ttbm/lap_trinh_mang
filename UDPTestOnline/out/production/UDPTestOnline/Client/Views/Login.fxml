<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Views.Login">
   <children>
      <TextField fx:id="txtUsername" layoutX="22.0" layoutY="145.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="22.0" layoutY="231.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
         <font>
            <Font size="16.0" />
         </font>
      </PasswordField>
      <Label layoutX="154.0" layoutY="37.0" text="LOGIN">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="120.0" text="Username">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="206.0" text="Password">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnLogin" layoutX="285.0" layoutY="326.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="32.0" prefWidth="100.0" text="Login">
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button fx:id="btnSignup" layoutX="172.0" layoutY="326.0" mnemonicParsing="false" onAction="#onSignup" prefHeight="32.0" prefWidth="100.0" text="Sign Up">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
